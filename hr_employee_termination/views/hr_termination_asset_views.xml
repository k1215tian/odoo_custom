<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View hr.termination.asset View List -->
    <record id="view_hr_termination_asset_list" model="ir.ui.view">
        <field name="name">view.hr.termination.asset.list</field>
        <field name="model">hr.termination.asset</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View hr.termination.asset form -->
    <record id="view_hr_termination_asset_form" model="ir.ui.view">
        <field name="name">view.hr.termination.asset.form</field>
        <field name="model">hr.termination.asset</field>
        <field name="arch" type="xml">
            <form string="Hr Termination Asset">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View hr.termination.asset search -->
    <record id="view_hr_termination_asset_search" model="ir.ui.view">
        <field name="name">view.hr.termination.asset.search</field>
        <field name="model">hr.termination.asset</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action hr.termination.asset -->
    <record id="action_hr_termination_asset" model="ir.actions.act_window">
        <field name="name">Hr Termination Asset</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">hr.termination.asset</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Hr Termination Asset.
            </p>
        </field>
    </record>

</odoo>
