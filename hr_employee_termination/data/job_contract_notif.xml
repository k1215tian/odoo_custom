<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Scheduled Action -->
        <record id="ir_cron_job_contract_notif" model="ir.cron">
            <field name="name">Job contract notif Scheduler</field>
            <field name="model_id" ref="model_job_contract_notif"/>
            <field name="state">code</field>
            <field name="code">
                model.method_to_run()
            </field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>