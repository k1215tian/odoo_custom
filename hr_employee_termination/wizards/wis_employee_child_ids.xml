<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View wis.employee.child_ids form -->
    <record id="view_wis_employee_child_ids_form" model="ir.ui.view">
        <field name="name">view.wis.employee.child_ids.form</field>
        <field name="model">wis.employee.child_ids</field>
        <field name="arch" type="xml">
            <form string="Wis Employee Child_ids">
                <div class="oe_title">
                    <label for="name"/>
                    <h1>
                        <field name="name" placeholder="Name..."/>
                    </h1>
                </div>
                <footer>
                    <button name="add" type="object" string="Add" class="oe_highlight"/>
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <!-- Action wis.employee.child_ids -->
    <record id="action_wis_employee_child_ids" model="ir.actions.act_window">
        <field name="name">Wis Employee Child_ids</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">wis.employee.child_ids</field>
        <field name="view_mode">form</field>
        <field name="binding_model_id" ref="addon.model_model_name"/>
        <field name="target">new</field>
    </record>

</odoo>
